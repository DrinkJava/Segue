#include "measure.h"



Measure::Measure(TimeSignature T, KeySignature K)
{
  Time_Sig = T;
  Key_Sig = K;
  int beatsLeft = T.beats;
  isFull = false;
  startNote = NULL;
  endNote = NULL;
}
